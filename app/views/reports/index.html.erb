<%= render "my-reports" %>

<div class="report-content" id="report-content">
  <% case @partial %>
  <% when 'grid' %>
      <%= render "grid" %>
  <% when 'data-view' %>
      <%= render "data-view" %>
  <% end %>
</div>
<div class="back-pop" id='back-pop'>
  <div class="choose-project-team">
    <i class="icon icon-remove" id="remove-back-pop"></i>
    <p>create discussion</p>
    <div class="choose-by-select">
      <p>Add to a discussion</p>

      <a class="btn btn-primary" id="add-to-storyset">Add</a>
    </div>
    <div class="choose-by-create">
      <p>Create new discussion</p>
      <input type="text" id="storyset-title" placeholder="discussion name"/>
      <input type="text" id="storyset-desc" placeholder="discussion description"/>

      <a class="btn btn-primary" id="create-add-to-storyset">Create and Add</a>
    </div>
  </div>
</div>
<% content_for :javascript_includes do %>
    <%= javascript_include_tag "func-module/format_time.js" %>
    <%= javascript_include_tag "preload/classie.js" %>
    <%= javascript_include_tag "preload/snap.svg-min.js" %>
    <%= javascript_include_tag "preload/svgLoader.js" %>
    <%= javascript_include_tag "xml2json"%>
    <%= javascript_include_tag "dhtmlx" %>
    <%= javascript_include_tag "dhtmlx-extend" %>
    <%= javascript_include_tag "reports/report.js" %>
    <%= javascript_include_tag "reports/main.js" %>
    <%= javascript_include_tag "reports/snap" %>
<% end %>

