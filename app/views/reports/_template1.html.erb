<%=stylesheet_link_tag "reports/template1" %>
<div class="template1">
  <section class="conditionr">
      <p>Condition</p>
  </section>
  <section class="content">

  </section>
  <section class="snaps" id="snap-groups">
    <% @snaps.each do |snap| %>
        <div class="snap-li" snap="<%= snap.id %>" type="<%= snap.type_string %>">
          <p><%= snap.desc %></p>
          <p><%= time_ago_in_words(snap.created_at.localtime) %> ago</p>
        </div>
    <% end %>
  </section>
</div>