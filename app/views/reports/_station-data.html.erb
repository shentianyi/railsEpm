<section class="station-data">
<div class="header" id="station-data-header">
  <div class="left">
    <p>Time interval</p>
    <input type="text" id="date-begin" readonly=true/>
    <label>-</label>
    <input type="text" id="date-end" readonly=true/>
    <br>
    <input type="button" value="Retrieve" id="retrieve" class="btn btn-primary"/>
  </div>
  <div class="right">
    <p>Export to</p>
    <a class="btn btn-success" onclick="export_bt_report_excel()">Excel</a>
  </div>
</div>
<div class="station-data-body" id="station-data-body">
  <div id="data_container" ></div>
</div>
<%= render 'snap'%>
</section>

<script>
$(document).ready(function(){
	Report.init("station_data");
})
</script>