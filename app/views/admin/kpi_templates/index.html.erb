<h1>Listing admin_kpi_templates</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Name</th>
    <th>Unit</th>
    <th>Frequency</th>
    <th>Target</th>
    <th>Is calculated</th>
    <th>Direction</th>
    <th>Period</th>
    <th>Formula</th>
    <th>Formula string</th>
    <th>Admin kpi category template</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_kpi_templates.each do |admin_kpi_template| %>
  <tr>
    <td><%= admin_kpi_template.description %></td>
    <td><%= admin_kpi_template.name %></td>
    <td><%= admin_kpi_template.unit %></td>
    <td><%= admin_kpi_template.frequency %></td>
    <td><%= admin_kpi_template.target %></td>
    <td><%= admin_kpi_template.is_calculated %></td>
    <td><%= admin_kpi_template.direction %></td>
    <td><%= admin_kpi_template.period %></td>
    <td><%= admin_kpi_template.formula %></td>
    <td><%= admin_kpi_template.formula_string %></td>
    <td><%= admin_kpi_template.admin_kpi_category_template %></td>
    <td><%= link_to 'Show', admin_kpi_template %></td>
    <td><%= link_to 'Edit', edit_admin_kpi_template_path(admin_kpi_template) %></td>
    <td><%= link_to 'Destroy', admin_kpi_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

