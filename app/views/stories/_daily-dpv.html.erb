    <div id="chart_dpv_one" class="chart_dpv_one"></div>
    <div id="chart_dpv_two" class="chart_dpv_two"></div>
  </div>
  <div class="right">
    <div id="data_container" class="data_container"></div>
  </div>
  <div>
    <input id=""hidden-data type="hidden" value="<%= @chart_conditions.first.data%>"/>
